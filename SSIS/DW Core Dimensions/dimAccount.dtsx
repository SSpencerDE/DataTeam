                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">;with base as (
select
AccountGUID = a.ACCOUNT_GUID,
AccountName = ISNULL(NULLIF(e.account,''),a.name),
AccountCity = ISNULL(f.city,a.city),
AccountRegion = ISNULL(left(f.state,2),c.REGION_CODE_ALPHA),
AccountPostalCode = case ISNULL(LEFT(F.COUNTRY,2),LEFT(a.country_code,2)) when 'US' 
                    then ISNULL(LEFT(F.POSTALCODE,5),left(a.Postal_code,5)) else ISNULL(F.POSTALCODE,a.postal_code) end,
AccountCountryCode = ISNULL(LEFT(F.COUNTRY,2),LEFT(a.country_code,2)),
AccountType = isnull(e.type,''),
row_number() over (partition by ACCOUNT_GUID order by isnull(a.update_DT,a.create_DT) desc) as rn
from 
EDU_SEC.dbo.account a 
left join EDU_SEC.dbo.SYSTEM_COUNTRY_REGION c on 
a.REGION_CODE = c.REGION_CODE and 
a.COUNTRY_CODE = c.COUNTRY_CODE
left join EDU_SEC.dbo.entity_property_setting d on 
a.ACCOUNT_GUID = d.ENTITY_GUID and
d.entity_property_guid ='E93509F0-9C9B-4554-9C98-23E4361195A3'
left join edu_sec_sales.dbo.account e on 
d.value = e.ACCOUNTID
left join edu_sec_sales.dbo.address f on 
e.addressid = f.ADDRESSID
)
SELECT 
AccountGUID,
AccountName = cast(isnull(AccountName,'') as nvarchar(255)),
AccountCity =  cast(isnull(AccountCity,'') as nvarchar(100)),
AccountRegion =  cast(isnull(AccountRegion,'') as nvarchar(256)),
AccountPostalCode =  cast(isnull(AccountPostalCode,'') as nvarchar(25)),
AccountCountryCode =  cast(isnull(AccountCountryCode,'') as nvarchar(2)),
AccountType =  cast(isnull(AccountType,'') as nvarchar(100)),
binary_Checksum(AccountGUID,AccountName,AccountCity,AccountRegion,AccountPostalCode,AccountCountryCode,AccountType) as binaryChecksum
FROM base
where rn = 1</property>

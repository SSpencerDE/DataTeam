			// TODO: Add your code here

			string body = (string)Dts.Variables["User::EmailBody"].Value;
			string subject = (string)Dts.Variables["User::EmailSubject"].Value;
			string from = (string)Dts.Variables["User::EmailFromAddress"].Value;
			string to = (string)Dts.Variables["User::EmailAddress"].Value;
			string server = (string)Dts.Variables["User::EmailServer"].Value;

            try
            {
                SendMailMessage(to, from, subject, body, true, server);
            }
            catch (Exception e)
            {
            }
			
			Dts.TaskResult = (int)ScriptResults.Success;
		}

		private void SendMailMessage(string SendTo, string From, string Subject, string Body, bool IsBodyHtml, string Server)
		{

			MailMessage htmlMessage = new MailMessage();
			SmtpClient mySmtpClient;

			htmlMessage.From = new MailAddress(From);
			foreach (string email in SendTo.Split(';'))
			{
				htmlMessage.To.Add(new MailAddress(email));
			}
			htmlMessage.Subject = Subject;
			htmlMessage.Body = Body;
			htmlMessage.IsBodyHtml = IsBodyHtml;
			
			mySmtpClient = new SmtpClient(Server);
			mySmtpClient.Credentials = CredentialCache.DefaultNetworkCredentials;
			mySmtpClient.Send(htmlMessage);
			
		}


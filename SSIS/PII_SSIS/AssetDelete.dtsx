declare 
	@AssetGuid		uniqueidentifier = ?,
	@ExecutionID	int = ?,
	@ErrorMessage	nvarchar(max) = ?

declare
	@ErrorStatusID int

select @ErrorStatusID = S.AssetStatusID from asset.[AssetStatus] S where S.[Name] = 'Error'

execute process.WriteLog
	@ExecutionID = @ExecutionID,
	@LogType = 'Error',
	@Message = 'An error occurred when trying to update tblAssetMetaData for this asset. Setting this asset record to [Error] Status. See following message for more details.',
	@Data = @AssetGuid

execute process.WriteLog
	@ExecutionID = @ExecutionID,
	@LogType = 'Error',
	@Message = @ErrorMessage,
	@Data = @AssetGuid

update A
	set A.AssetStatusID = @ErrorStatusID
	from
		asset.Asset A
	where
		A.AssetGuid = @AssetGuid</property>
